<table>
  <tr>
    <th>User</th>
    <th>Time of reaching the Find Page</th>
    <th>Time of reaching the Detail Page</th>
    <th>Difference</th>
  </tr>
  <% @times.each do |t| %>
    <tr>
      <td><%= t.user.name %></td>
      <td><%= t.find_timestamp %></td>
      <td><%= t.detail_timestamp %></td>
      <td><%= t.difference %></td>            
    </tr>
  <% end %>
</table>